actor Source_Smaller2Bigger () ==> uint(size=8) OP:

	bool dataSent := false;
	
	action ==> OP:[0x00,0x01,0x02,0x03,
				   0x04,0x05,0x06,0x07,
				   0x08,0x09,0x0A,0x0B,
				   0x0C,0x0D,0x0E,0x0F
				   ]
