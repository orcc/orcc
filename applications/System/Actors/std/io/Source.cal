package std.io;


actor Source()
		==> uint(size=8) O
		:
		
	native procedure source_init() end
	native function source_read_file() --> uint(size=8) end
	
	initialize ==>
	do
		source_init();
	end
	
	read: action ==> O : [source_read_file()]
	end
	
end