    readpix ( read_case_0      )    --> write;
    readpix ( read_case_1      )    --> write;
    readpix ( read_case_2      )    --> write;
    readpix ( read_case_3      )    --> write;    

    write   ( write                )    --> start;
  end
  
  priority
    read_case_0 > read_case_1 >read_case_2 >read_case_3;
