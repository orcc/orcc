actor idct2d_print () input ==> :

  action input:[values] repeat 64 ==>
  var
    i, j,
    list = [ values[k] : for k in Integers(0,63) ]
  do
    i := 0;
    while i < 8 do
      j := 0;
      while j < 8 do
        println("value: " + values[i * 8 + j]);
        j := j + 1;
      end
      i := i + 1;
    end
  end

end
